<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".admin.AdminActivity">

    <Button
        android:id="@+id/button_add_category"
        android:layout_width="273dp"
        android:layout_height="59dp"
        android:layout_marginTop="288dp"
        android:text="@string/add_category"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email_admin"
        app:layout_constraintVertical_bias="0.046" />

    <TextView
        android:id="@+id/email_admin"
        android:layout_width="251dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="email"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="@id/welcome_admin"
        app:layout_constraintTop_toBottomOf="@+id/welcome_admin" />

    <TextView
        android:id="@+id/welcome_admin"
        style="@style/Text.Title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:text="Welcome,"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.158"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.689" />
</androidx.constraintlayout.widget.ConstraintLayout>